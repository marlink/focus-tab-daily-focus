
{
  "manifest_version": 3,
  "name": "FocusTab",
  "version": "1.0.0",
  "description": "A minimal Chrome extension that keeps your day's most important tasks front and center",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ]
}
